<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ColoredScale">
        <attr name="label" format="string" />
        <attr name="labelColor" format="color" />
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
        <attr name="graduationStartColor" format="color" />
        <attr name="graduationEndColor" format="color" />
        <attr name="labelHeight" format="dimension" />
        <attr name="startValue" format="float" />
        <attr name="endValue" format="float" />
        <attr name="graduationStep" format="float" />
        <attr name="showText" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SeekBarPreference">
        <attr name="minValue" format="reference|integer"></attr>
        <attr name="maxValue" format="reference|integer"></attr>
        <attr name="unitsRight" format="reference|string"></attr>
        <attr name="unitsLeft" format="reference|string"></attr>
        <attr name="interval" format="reference|integer"></attr>
    </declare-styleable>

</resources>