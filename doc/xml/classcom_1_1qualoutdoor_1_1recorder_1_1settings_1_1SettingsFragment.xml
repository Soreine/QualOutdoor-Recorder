        @Override
        public void onSharedPreferenceChanged(
                SharedPreferences sharedPreferences, String key) {

            if (key.equals(getString(R.string.pref_key_minimum_upload_size))) {
                
                <ref refid="classcom_1_1qualoutdoor_1_1recorder_1_1settings_1_1SettingsFragment_1a10c070e465872c5cdccf51c7bf975501" kindref="member">updateMinimumUploadSizeSummary</ref>(sharedPreferences);
            }
            if (key.equals(getString(R.string.pref_key_network_policy))) {
                
                <ref refid="classcom_1_1qualoutdoor_1_1recorder_1_1settings_1_1SettingsFragment_1afa4141070c837fc5934be099fcb4ce20" kindref="member">updateNetworkPolicySummary</ref>(sharedPreferences);
            }
            if (key.equals(getString(R.string.pref_key_protocol))) {
                
                <ref refid="classcom_1_1qualoutdoor_1_1recorder_1_1settings_1_1SettingsFragment_1a224b8945815532c1943d515223816d95" kindref="member">updateProtocolSummary</ref>(sharedPreferences);
            }

        }

    }</initializer>
